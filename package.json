{
  "name": "andrewstiefel.com",
  "version": "1.0.0",
  "description": "Personal website built with Jekyll and Tailwind CSS",
  "author": "Andrew Stiefel",
  "license": "MIT",
  "scripts": {
    "build": "npm run uglify && jekyll build",
    "preinstall": "cd netlify/functions && npm install",
    "start": "bundle exec jekyll serve --livereload",
    "uglify": "uglifyjs _site/assets/js/bundle.js -c -o _site/assets/js/bundle.min.js",
    "configure": "bundle install & npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewstiefel/andrewstiefel.com"
  },
"devDependencies": {
  "uglify-js": "^3.19.3"
  }
}